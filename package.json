{
  "name": "ant-design-zero",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "serve": "vite preview",
    "prepare": "husky install",
    "format": "prettier --write src",
    "lint": "tsc --noEmit && eslint --ext .ts,.tsx ./src",
    "lint:fix": "tsc --noEmit && eslint --fix --cache --ext .ts,.tsx ./src",
    "clean": "rimraf dist node_modules yarn.lock package.lock.json .eslintcache",
    "clean:cache": "rimraf node_modules/.cache/ node_modules/.vite .eslintcache"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn lint",
    "**/*.{tsx,ts,less,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-form": "^1.52.3",
    "antd": "^4.17.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.5",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-react": "^1.1.3",
    "eslint": "^8.5.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.7",
    "vite-plugin-imp": "^2.0.11"
  }
}
